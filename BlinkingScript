 public GameObject BlinkPanel;
 void Start()
 {
     StartCoroutine(CastBlink());
 }
 IEnumerator CastBlink()
 {
     while (true)
     {
         BlinkPanel.SetActive(false);
         yield return new WaitForSeconds(10);
         BlinkPanel.SetActive(true);
         yield return new WaitForSeconds(1);
     }
 }
